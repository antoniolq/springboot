<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="student" class="org.example.entity.Student">
        <property name="age" value="1"></property>
        <property name="id" value="1"></property>
        <property name="name" value="za"></property>
    </bean>

    <bean id="student2" class="org.example.entity.Student">
        <constructor-arg name="age" value="1"></constructor-arg>
        <constructor-arg name="id" value="1"></constructor-arg>
        <constructor-arg name="name" value="as"></constructor-arg>
    </bean>
</beans>
